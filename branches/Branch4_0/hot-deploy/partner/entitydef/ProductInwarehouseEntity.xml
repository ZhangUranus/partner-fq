<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<!-- ========================================================= -->
	<!-- ======================== Defaults ======================= -->
	<!-- ========================================================= -->
	<title>Entity of an Open For Business Project Component</title>
	<description>None</description>
	<copyright>Copyright 2001-2009 The Apache Software Foundation</copyright>
	<author>None</author>
	<version>1.0</version>
	<!-- 表头 -->
	<entity entity-name="ProductInwarehouse" package-name="org.ofbiz.scm.ProductInwarehouse" title="ProductInwarehouse Entity">
		<field name="id" type="id-long"></field>
		<field name="number" type="name"></field>
		<field name="bizDate" type="date-time"></field>
		<field name="inspectorSystemUserId" type="id-long"></field>
		<field name="submitterSystemUserId" type="id-long"></field>
		<field name="totalsum" type="fixed-point"></field>
		<field name="note" type="description"></field>
		<field name="status" type="int"></field>
		<field name="billType" type="int"></field>		<!--1.普通单据，2.扫描单据-->
		<field name="createdStamp" type="date-time"></field>
		<field name="lastUpdatedStamp" type="date-time"></field>
		<field name="submitStamp" type="date-time"></field>
		<prim-key field="id" />
	</entity>
	<!-- 分录 -->
	<entity entity-name="ProductInwarehouseEntry" package-name="org.ofbiz.scm.ProductInwarehouse" title="ProductInwarehouseEntry Entity">
		<field name="id" type="id-long"></field>
		<field name="parentId" type="id-long"></field>
		<field name="productWeek" type="value"></field>
		<field name="workshopWorkshopId" type="id-long"></field>
		<field name="warehouseWarehouseId" type="id-long"></field>
		<field name="materialMaterialId" type="id-long"></field>
		<field name="volume" type="fixed-point"></field>
		<field name="unitUnitId" type="id-long"></field>
		<field name="price" type="fixed-point"></field>
		<field name="entrysum" type="fixed-point"></field>
		<field name="barcode1" type="value"></field>
		<field name="barcode2" type="value"></field>
		<field name="qantity" type="numeric"></field><!-- 板数量 -->
		<field name="inwarehouseType" type="value"></field>
		<field name="sort" type="int"></field>
		<field name="lastUpdatedStamp" type="date-time"></field>
		<prim-key field="id" />
	</entity>
	<!-- 进仓单分录额外耗料表 记录进仓单每一个成品的实际耗料数 -->
	<entity entity-name="ProductInwarehouseEntryDetail" package-name="org.ofbiz.scm.ProductInwarehouse" title="ProductInwarehouseEntryDetail Entity">
		<field name="id" type="id-long"></field>
		<field name="parentId" type="id-long"></field>
		<field name="barcode1" type="value"></field><!-- 为提供处理销量增加 -->
		<field name="barcode2" type="value"></field><!-- 为提供处理销量增加 -->
		<field name="materialId" type="id-long"></field>
		<field name="model" type="value"></field>
		<field name="quantity" type="fixed-point"></field>
		<field name="unitUnitId" type="id-long"></field>
		<field name="price" type="fixed-point"></field>
		<field name="amount" type="fixed-point"></field>
		<prim-key field="id" />
		<index name="PRT_INS_ENR_DTL_PTI" unique="false">
            <index-field name="parentId"/>
		</index>
		<index name="PRT_INS_ENR_DTL_BARC" unique="false">
            <index-field name="barcode1"/>
            <index-field name="barcode2"/>
		</index>
	</entity>
	<!-- 表头视图 -->
	<view-entity entity-name="ProductInwarehouseView" package-name="org.ofbiz.scm.ProductInwarehouse" title="ProductInwarehouseView  Entity">
		<member-entity entity-alias="ProductInwarehouseV" entity-name="ProductInwarehouse"></member-entity>
		<member-entity entity-alias="ProductInwarehouseEntryV" entity-name="ProductInwarehouseEntry"></member-entity>
		<member-entity entity-alias="materialMaterialV" entity-name="TMaterial"></member-entity>
		<member-entity entity-alias="inspectorSystemUserV" entity-name="TSystemUser"></member-entity>
		<member-entity entity-alias="submitterSystemUserV" entity-name="TSystemUser"></member-entity>
		<alias-all entity-alias="ProductInwarehouseV"></alias-all>
		<alias name="materialMaterialId" entity-alias="ProductInwarehouseEntryV" field="materialMaterialId"></alias>
		<alias name="inspectorSystemUserName" entity-alias="inspectorSystemUserV" field="userName"></alias>
		<alias name="submitterSystemUserName" entity-alias="submitterSystemUserV" field="userName"></alias>
		
		<!-- 查询条件需要取物料编码、物料名称，在查询数据中需要distinct -->
		<view-link entity-alias="ProductInwarehouseV" rel-entity-alias="ProductInwarehouseEntryV" rel-optional="false">
			<key-map field-name="id" rel-field-name="parentId" />
		</view-link>
		<view-link entity-alias="ProductInwarehouseEntryV" rel-entity-alias="materialMaterialV" rel-optional="true">
			<key-map field-name="materialMaterialId" rel-field-name="id" />
		</view-link>
		<!-- 查询条件需要取物料编码、物料名称，在查询数据中需要distinct -->
		
		<view-link entity-alias="ProductInwarehouseV" rel-entity-alias="inspectorSystemUserV" rel-optional="true">
			<key-map field-name="inspectorSystemUserId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="ProductInwarehouseV" rel-entity-alias="submitterSystemUserV" rel-optional="true">
			<key-map field-name="submitterSystemUserId" rel-field-name="id" />
		</view-link>
	</view-entity>
	<!-- 导出视图 -->
	<view-entity entity-name="ProductInwarehouseExportView" package-name="org.ofbiz.scm.ConsignWarehousing" title="ProductInwarehouseExportView  Entity">
		<member-entity entity-alias="ProductInwarehouseV" entity-name="ProductInwarehouse"></member-entity>
		<member-entity entity-alias="ProductInwarehouseEntryV" entity-name="ProductInwarehouseEntry"></member-entity>
		<member-entity entity-alias="materialMaterialV" entity-name="TMaterial"></member-entity>
		<member-entity entity-alias="inspectorSystemUserV" entity-name="SystemUser"></member-entity>
		<member-entity entity-alias="submitterSystemUserV" entity-name="SystemUser"></member-entity>
		<alias-all entity-alias="ProductInwarehouseV"></alias-all>
		<alias name="inspectorSystemUserName" entity-alias="inspectorSystemUserV" field="name"></alias>
		<alias name="submitterSystemUserName" entity-alias="submitterSystemUserV" field="name"></alias>
		
		<!-- 查询条件需要取物料编码、物料名称，在查询数据中需要distinct -->
		<view-link entity-alias="ProductInwarehouseV" rel-entity-alias="ProductInwarehouseEntryV" rel-optional="true">
			<key-map field-name="id" rel-field-name="parentId" />
		</view-link>
		<view-link entity-alias="ProductInwarehouseEntryV" rel-entity-alias="materialMaterialV" rel-optional="true">
			<key-map field-name="materialMaterialId" rel-field-name="id" />
		</view-link>
		<!-- 查询条件需要取物料编码、物料名称，在查询数据中需要distinct -->
		
		<view-link entity-alias="ProductInwarehouseV" rel-entity-alias="inspectorSystemUserV" rel-optional="true">
			<key-map field-name="inspectorSystemUserId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="ProductInwarehouseV" rel-entity-alias="submitterSystemUserV" rel-optional="true">
			<key-map field-name="submitterSystemUserId" rel-field-name="id" />
		</view-link>
	</view-entity>
	<!--表体视图 -->
	<view-entity entity-name="ProductInwarehouseEntryView" package-name="org.ofbiz.scm.ProductInwarehouse" title="ProductInwarehouseEntryView  Entity">
		<member-entity entity-alias="ProductInwarehouseEntryV" entity-name="ProductInwarehouseEntry"></member-entity>
		<member-entity entity-alias="workshopWorkshopV" entity-name="Workshop"></member-entity>
		<member-entity entity-alias="warehouseWarehouseV" entity-name="Warehouse"></member-entity>
		<member-entity entity-alias="materialMaterialV" entity-name="TMaterial"></member-entity>
		<member-entity entity-alias="unitUnitV" entity-name="Unit"></member-entity>
		<alias-all entity-alias="ProductInwarehouseEntryV"></alias-all>
		<alias name="workshopWorkshopName" entity-alias="workshopWorkshopV" field="name"></alias>
		<alias name="warehouseWarehouseName" entity-alias="warehouseWarehouseV" field="name"></alias>
		<alias name="materialMaterialName" entity-alias="materialMaterialV" field="name"></alias>
		<alias name="materialModel" entity-alias="materialMaterialV" field="model"></alias>
		<alias name="unitUnitName" entity-alias="unitUnitV" field="name"></alias>
		<view-link entity-alias="ProductInwarehouseEntryV" rel-entity-alias="workshopWorkshopV" rel-optional="true">
			<key-map field-name="workshopWorkshopId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="ProductInwarehouseEntryV" rel-entity-alias="warehouseWarehouseV" rel-optional="true">
			<key-map field-name="warehouseWarehouseId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="ProductInwarehouseEntryV" rel-entity-alias="materialMaterialV" rel-optional="true">
			<key-map field-name="materialMaterialId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="ProductInwarehouseEntryV" rel-entity-alias="unitUnitV" rel-optional="true">
			<key-map field-name="unitUnitId" rel-field-name="id" />
		</view-link>
	</view-entity>
	<!-- 分录明细耗料视图 -->
	<view-entity entity-name="ProductInwarehouseEntryDetailView" package-name="org.ofbiz.scm.ProductInwarehouse" title="ProductInwarehouseEntryDetailView  Entity">
		<member-entity entity-alias="ProductInwarehouseEntryDetailV" entity-name="ProductInwarehouseEntryDetail"></member-entity>
		<member-entity entity-alias="materialMaterialV" entity-name="TMaterial"></member-entity>
		<member-entity entity-alias="unitUnitV" entity-name="Unit"></member-entity>
		<alias-all entity-alias="ProductInwarehouseEntryDetailV"></alias-all>
		<alias name="materialMaterialName" entity-alias="materialMaterialV" field="name"></alias>
		<alias name="unitUnitName" entity-alias="unitUnitV" field="name"></alias>
		<view-link entity-alias="ProductInwarehouseEntryDetailV" rel-entity-alias="materialMaterialV" rel-optional="true">
			<key-map field-name="materialId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="ProductInwarehouseEntryDetailV" rel-entity-alias="unitUnitV" rel-optional="true">
			<key-map field-name="unitUnitId" rel-field-name="id" />
		</view-link>
	</view-entity>
</entitymodel>