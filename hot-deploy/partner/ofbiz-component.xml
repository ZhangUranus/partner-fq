<?xml version="1.0" encoding="UTF-8"?>
<ofbiz-component name="partner"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.ofbiz.org/dtds/ofbiz-component.xsd">
	<resource-loader name="main" type="component"/>
	<classpath type="jar" location="build/lib/*"/>
	<classpath type="jar" location="lib/*"/>
	<entity-resource type="model" reader-name="main" loader="main" location="entitydef/entitymodel.xml"/>
	<entity-resource type="model" reader-name="main" loader="main" location="entitydef/bdentitymodel.xml"/>
	<entity-resource type="model" reader-name="main" loader="main" location="entitydef/PurchaseBillEntity.xml"/>
    <entity-resource type="data" reader-name="seed" loader="main" location="data/InitData.xml"/>
	<webapp name="scm" 
		title="SCM" 
		server="default-server" 
		location="webapp/scm" 
		base-permission="NONE" 
		mount-point="/scm"/>
		
	<webapp name="scmui"
        title="SCMUI"
        server="default-server"
        location="webapp/scmui"
        mount-point="/scmui"
        app-bar-display="false"/>
        
</ofbiz-component>
