<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<!-- ========================================================= -->
	<!-- ======================== Defaults ======================= -->
	<!-- ========================================================= -->
	<title>Entity of an Open For Business Project Component</title>
	<description>None</description>
	<copyright>Copyright 2001-2009 The Apache Software Foundation</copyright>
	<author>None</author>
	<version>1.0</version>
	<!-- 表头 -->
	<entity entity-name="PurchaseBill" package-name="org.ofbiz.scm.PurchaseBill" title="PurchaseBill Entity">
		<field name="id" type="id-long"></field>
		<field name="number" type="name"></field>
		<field name="bizDate" type="date-time"></field>
		<field name="supplierSupplierId" type="id-long"></field>
		<field name="submitUserId" type="id-long"></field>
		<field name="buyerSystemUserId" type="id-long"></field>
		<field name="approverSystemUserId" type="id-long"></field>
		<field name="totalsum" type="fixed-point"></field>
		<field name="receiveStamp" type="date-time"></field>
		<field name="payType" type="value"></field><!-- 付款方式 -->
		<field name="tax" type="value"></field><!--税金  -->
		<field name="deliveryAddr" type="description"></field><!-- 送货地址 -->
		<field name="revision" type="value"></field><!--revision  -->
		<field name="note" type="description"></field>
		<field name="approverNote" type="description"></field>
		<field name="status" type="int"></field>
		<field name="createdStamp" type="date-time"></field>
		<field name="lastUpdatedStamp" type="date-time"></field>
		<prim-key field="id" />
	</entity>
	<!-- 分录 --> 
	<entity entity-name="PurchaseBillEntry" package-name="org.ofbiz.scm.PurchaseBill" title="PurchaseBillEntry Entity">
		<field name="id" type="id-long"></field>
		<field name="parentId" type="id-long"></field>
		<field name="materialMaterialId" type="id-long"></field>
		<field name="materialMaterialModel" type="name"></field>
		<field name="volume" type="fixed-point"></field>
		<field name="unitUnitId" type="id-long"></field>
		<field name="price" type="fixed-point"></field>
		<field name="refPrice" type="fixed-point"></field>
		<field name="entrysum" type="fixed-point"></field>
		<field name="sort" type="int"></field>
		<field name="qualityReq" type="value"></field><!--质量要求  -->
		<field name="deliveryDate" type="date-time"></field><!--交货日期  -->
		<field name="deliveryQty" type="fixed-point"></field><!-- 交货数量 -->
		<field name="warehouseId" type="id-long"></field><!-- 仓库 -->
		<field name="address" type="value"></field><!-- 地点-->
		<prim-key field="id" />
	</entity>
	<!-- 表头视图 -->
	<view-entity entity-name="PurchaseBillView" package-name="org.ofbiz.scm.PurchaseBill" title="PurchaseBillView  Entity">
		<member-entity entity-alias="PurchaseBillV" entity-name="PurchaseBill"></member-entity>
		<member-entity entity-alias="PurchaseBillEntryV" entity-name="PurchaseBillEntry"></member-entity>
		<member-entity entity-alias="materialMaterialV" entity-name="TMaterial"></member-entity>
		<member-entity entity-alias="supplierSupplierV" entity-name="Supplier"></member-entity>
		<member-entity entity-alias="submitSystemUserV" entity-name="SystemUser"></member-entity>
		<member-entity entity-alias="buyerSystemUserV" entity-name="SystemUser"></member-entity>
		<member-entity entity-alias="buyerDepartmentV" entity-name="Department"></member-entity>
		<member-entity entity-alias="approverSystemUserV" entity-name="SystemUser"></member-entity>
		
		<alias-all entity-alias="PurchaseBillV"></alias-all>
		<alias name="materialMaterialId" entity-alias="PurchaseBillEntryV" field="materialMaterialId"></alias>
		<alias name="entrySeq" entity-alias="PurchaseBillEntryV" field="sort" ></alias>
		<alias name="supplierSupplierName" entity-alias="supplierSupplierV" field="name"></alias>
		<alias name="supplierSupplierPhoneNum" entity-alias="supplierSupplierV" field="phoneNum"></alias>
		<alias name="supplierSupplierFaxNum" entity-alias="supplierSupplierV" field="faxNum"></alias>
		<alias name="supplierSupplierAddress" entity-alias="supplierSupplierV" field="address"></alias>
		<alias name="supplierSupplierLinkman" entity-alias="supplierSupplierV" field="linkman"></alias>
		<alias name="supplierSupplierEmail" entity-alias="supplierSupplierV" field="email"></alias>
		<alias name="submitUserName" entity-alias="submitSystemUserV" field="name"></alias>
		<alias name="buyerSystemUserName" entity-alias="buyerSystemUserV" field="name"></alias>
		<alias name="buyerDepartmentId" entity-alias="buyerDepartmentV" field="id"></alias>
		<alias name="buyerDepartmentName" entity-alias="buyerDepartmentV" field="name"></alias>
		<alias name="approverSystemUserName" entity-alias="approverSystemUserV" field="name"></alias>
		
		<!-- 查询条件需要取物料编码、物料名称，在查询数据中需要distinct -->
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="PurchaseBillEntryV" rel-optional="true">
			<key-map field-name="id" rel-field-name="parentId" />
		</view-link>
		<view-link entity-alias="PurchaseBillEntryV" rel-entity-alias="materialMaterialV" rel-optional="true">
			<key-map field-name="materialMaterialId" rel-field-name="id" />
		</view-link>
		<!-- 查询条件需要取物料编码、物料名称，在查询数据中需要distinct -->
		
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="supplierSupplierV" rel-optional="true">
			<key-map field-name="supplierSupplierId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="submitSystemUserV" rel-optional="true">
			<key-map field-name="submitUserId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="buyerSystemUserV" rel-optional="true">
			<key-map field-name="buyerSystemUserId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="buyerSystemUserV" rel-entity-alias="buyerDepartmentV" rel-optional="true">
			<key-map field-name="departmentId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="approverSystemUserV" rel-optional="true">
			<key-map field-name="approverSystemUserId" rel-field-name="id" />
		</view-link>
		
	</view-entity>
	<!-- 导出视图 -->
	<view-entity entity-name="PurchaseBillExportView" package-name="org.ofbiz.scm.PurchaseBill" title="PurchaseBillExportView  Entity">
		<member-entity entity-alias="PurchaseBillV" entity-name="PurchaseBill"></member-entity>
		<member-entity entity-alias="PurchaseBillEntryV" entity-name="PurchaseBillEntry"></member-entity>
		<member-entity entity-alias="materialMaterialV" entity-name="TMaterial"></member-entity>
		<member-entity entity-alias="supplierSupplierV" entity-name="Supplier"></member-entity>
		<member-entity entity-alias="submitSystemUserV" entity-name="SystemUser"></member-entity>
		<member-entity entity-alias="buyerSystemUserV" entity-name="SystemUser"></member-entity>
		<member-entity entity-alias="approverSystemUserV" entity-name="SystemUser"></member-entity>
		<alias-all entity-alias="PurchaseBillV"></alias-all>
		<alias name="supplierSupplierName" entity-alias="supplierSupplierV" field="name"></alias>
		<alias name="submitUserName" entity-alias="submitSystemUserV" field="name"></alias>
		<alias name="buyerSystemUserName" entity-alias="buyerSystemUserV" field="name"></alias>
		<alias name="approverSystemUserName" entity-alias="approverSystemUserV" field="name"></alias>
		
		<!-- 查询条件需要取物料编码、物料名称，在查询数据中需要distinct -->
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="PurchaseBillEntryV" rel-optional="true">
			<key-map field-name="id" rel-field-name="parentId" />
		</view-link>
		<view-link entity-alias="PurchaseBillEntryV" rel-entity-alias="materialMaterialV" rel-optional="true">
			<key-map field-name="materialMaterialId" rel-field-name="id" />
		</view-link>
		<!-- 查询条件需要取物料编码、物料名称，在查询数据中需要distinct -->
		
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="supplierSupplierV" rel-optional="true">
			<key-map field-name="supplierSupplierId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="submitSystemUserV" rel-optional="true">
			<key-map field-name="submitUserId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="buyerSystemUserV" rel-optional="true">
			<key-map field-name="buyerSystemUserId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="PurchaseBillV" rel-entity-alias="approverSystemUserV" rel-optional="true">
			<key-map field-name="approverSystemUserId" rel-field-name="id" />
		</view-link>
	</view-entity>
	<!--表体视图 -->
	<view-entity entity-name="PurchaseBillEntryView" package-name="org.ofbiz.scm.PurchaseBill" title="PurchaseBillEntryView  Entity">
		<member-entity entity-alias="PurchaseBillEntryV" entity-name="PurchaseBillEntry"></member-entity>
		<member-entity entity-alias="materialMaterialV" entity-name="TMaterial"></member-entity>
		<member-entity entity-alias="unitUnitV" entity-name="Unit"></member-entity>
		<member-entity entity-alias="warehouseV" entity-name="Warehouse"></member-entity>
		<alias-all entity-alias="PurchaseBillEntryV"></alias-all>
		<alias name="materialMaterialName" entity-alias="materialMaterialV" field="name" ></alias>
		<alias name="materialMaterialNumber" entity-alias="materialMaterialV" field="number"></alias>
		<alias name="materialMaterialModel" entity-alias="materialMaterialV" field="model"></alias>
		<alias name="materialMaterialQuality" entity-alias="materialMaterialV" field="quality"></alias>
		<alias name="materialMtarialDescription" entity-alias="materialMaterialV" field="description"></alias>
		<alias name="unitUnitName" entity-alias="unitUnitV" field="name"></alias>
		<alias name="warehouseName" entity-alias="warehouseV" field="name"></alias>
		
		<view-link entity-alias="PurchaseBillEntryV" rel-entity-alias="materialMaterialV" rel-optional="true">
			<key-map field-name="materialMaterialId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="PurchaseBillEntryV" rel-entity-alias="unitUnitV" rel-optional="true">
			<key-map field-name="unitUnitId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="PurchaseBillEntryV" rel-entity-alias="warehouseV" rel-optional="true">
			<key-map field-name="warehouseId" rel-field-name="id" />
		</view-link>
		<entity-condition> <order-by field-name="sort ASC" /> </entity-condition>
	</view-entity>
</entitymodel>