<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<!-- ========================================================= -->
	<!-- ======================== Defaults ======================= -->
	<!-- ========================================================= -->
	<title>Entity of an Open For Business Project Component</title>
	<description>None</description>
	<copyright>Copyright 2001-2009 The Apache Software Foundation</copyright>
	<author>None</author>
	<version>1.0</version>
	<!-- 产品扫描中间表 -->
	<entity entity-name="ProductScan" package-name="org.ofbiz.scm.scan" title="ProductScan Entity">
		<field name="id" type="id-long"></field><!-- 产品编码 -->
		<field name="productId" type="id-long"></field><!-- 产品编码 -->
		<field name="bizDate" type="date-time"></field><!-- 日期 -->
		<field name="qantityC" type="very-short"></field><!-- 板数量(字符) -->
		<field name="barcode1" type="value"></field><!-- 条码 -->
		<field name="barcode2" type="value"></field><!-- 流水码 -->
		<field name="type" type="int"></field><!-- 类型（减或调拨单性质的出写入为-1,其余为+1） -->
		<field name="volume" type="fixed-point"></field><!-- 数量（代表每次扫描一个托板，基本值为1） -->
		<field name="inOutType" type="indicator"></field><!-- 出入类别（A正常出入，B非正常出入） -->
		<field name="description" type="description"></field><!-- 描述（产品范围，客户要求的，本系统用于区分包装类型的文字描述） -->
		<field name="qantity" type="numeric"></field><!-- 板数量(数字) -->
		<field name="warehouseType" type="int"></field><!-- 交易类别(按单据性质的交易类别判断码(1.入库,2.销货,3.领用,4.调拨,5.调整)) -->
		<field name="status" type="int"></field><!-- 状态(0：待处理，1：处理中，2：已处理) -->
		<prim-key field="id" />
	</entity>
</entitymodel>