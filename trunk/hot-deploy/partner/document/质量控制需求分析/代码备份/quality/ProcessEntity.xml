<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">
	<title>Entity of an Open For Business Project Component</title>
	<description>None</description>
	<copyright>Copyright 2001-2009 The Apache Software Foundation</copyright>
	<author>None</author>
	<version>1.0</version>

	<!-- 表头 -->
	<entity entity-name="Process" package-name="org.ofbiz.scm.quality.Process" title="Process Entity">
		<field name="id" type="id-long"></field>
		<field name="number" type="name"></field>
		<field name="name" type="value"></field>
		<field name="creatorId" type="id-long"></field>
		<field name="lastUpdaterId" type="id-long"></field>
		<field name="description" type="description"></field>
		<field name="createdStamp" type="date-time"></field>
		<field name="lastUpdatedStamp" type="date-time"></field>
		<prim-key field="id" />
	</entity>

	<view-entity entity-name="ProcessView" package-name="org.ofbiz.scm.quality.Process"
		title="ProcessView  Entity">
		<member-entity entity-alias="ProcessV" entity-name="Process"></member-entity>
		<member-entity entity-alias="creatorUserV" entity-name="SystemUser"></member-entity>
		<member-entity entity-alias="lastUpdaterUserV"
			entity-name="SystemUser"></member-entity>
		<alias-all entity-alias="ProcessV"></alias-all>
		<alias name="creatorName" entity-alias="creatorUserV" field="name"></alias>
		<alias name="lastUpdaterName" entity-alias="lastUpdaterUserV"
			field="name"></alias>

		<view-link entity-alias="ProcessV" rel-entity-alias="creatorUserV"
			rel-optional="true">
			<key-map field-name="creatorId" rel-field-name="id" />
		</view-link>
		<view-link entity-alias="ProcessV" rel-entity-alias="lastUpdaterUserV"
			rel-optional="true">
			<key-map field-name="lastUpdaterId" rel-field-name="id" />
		</view-link>
	</view-entity>
</entitymodel>
    